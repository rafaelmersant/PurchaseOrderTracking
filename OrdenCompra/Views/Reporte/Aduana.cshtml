@model IEnumerable<OrdenCompra.Models.ReportAduana_Result>

@{
    ViewBag.Title = "Reporte de Aduana";
}

<h4 class="mt-3 ml-3">Reporte de Aduana (Mercancia por llegar)</h4>

<section class="container-fluid">
    <table id="articlesTable" class="table table-striped table-bordered table-condensed table-sm">
        <thead>
            <tr class="bg-danger text-light">
                <th>
                    Suplidor
                </th>
                <th class="text-center">
                    Cantidad
                </th>
                <th>
                    Articulo
                </th>
                <th>
                    Modelo
                </th>
                <th class="text-center">
                    BL
                </th>
                <th class="text-center">
                    Contenedores
                </th>
                <th class="text-center">
                    Fecha llegada
                </th>
                <th class="text-center">
                    Vence E/F
                </th>
                <th class="text-center">
                    Naviera
                </th>
                <th class="text-center">
                    Situación
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ProviderName</td>
                    <td>@item.QuantityTraffic</td>
                    <td>@item.ArticleDescription</td>
                    <td>@item.Model</td>
                    <td class="text-center">@item.BL</td>
                    <td class="text-center">@item.Contenedores</td>
                    <td class="text-center">@item.DueDate</td>
                    <td class="text-center">@item.VenceEF</td>
                    <td class="text-center">@item.Naviera</td>
                    <td class="text-center">
                        <input type="text" name="situation" value="" class="form-control form-control-sm"/>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</section>